<?import javafx.scene.layout.VBox?>

<GridPane alignment="center" hgap="10" prefHeight="400.0" prefWidth="600.0" vgap="10" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rolandVoxes.internetAPIBenchmark.ui.MainWindowController">
<columnConstraints>
    <ColumnConstraints />
</columnConstraints>
<rowConstraints>
    <RowConstraints />
</rowConstraints>
<children>
    <TabPane fx:id="appTabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
        <tabs>
            <Tab fx:id="newTestPane" text="New test">
                <content>
                    <SplitPane dividerPositions="0.5005411255411255" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
                        <items>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="118.0" prefWidth="600.0">
                                <children>
                                    <VBox prefHeight="218.0" prefWidth="600.0" spacing="5.0">
                                        <children>
                                            <HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="579.0">
                                                <children>
                                                    <Label fx:id="urlRequestLabel" text="Url: " textAlignment="CENTER">
                                                        <HBox.margin>
                                                            <Insets right="10.0" />
                                                        </HBox.margin>
                                                    </Label>
                                                    <TextField fx:id="urlRequestText" promptText="Write api url here..." HBox.hgrow="ALWAYS" />
                                                </children>
                                            </HBox>
                                            <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" prefHeight="30.0" prefWidth="579.0">
                                                <children>
                                                    <Label fx:id="countRequestLabel" text="How many requests:" textAlignment="CENTER">
                                                        <HBox.margin>
                                                            <Insets right="10.0" />
                                                        </HBox.margin>
                                                    </Label>
                                                    <TextField fx:id="countRequestText" HBox.hgrow="ALWAYS" />
                                                </children>
                                            </HBox>
                                            <Button fx:id="startBenchmarkButton" mnemonicParsing="false" onAction="#startBenchmark" text="Start" />
                                        </children>
                                        <padding>
                                            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                        </padding>
                                    </VBox>
                                </children>
                                <opaqueInsets>
                                    <Insets />
                                </opaqueInsets>
                            </AnchorPane>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                                <children>
                                    <TextArea fx:id="resultTextArea" disable="true" prefHeight="181.0" prefWidth="543.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                </children>
                            </AnchorPane>
                        </items>
                    </SplitPane>
                </content>
            </Tab>
            <Tab fx:id="historyPane" text="History">
                <content>
                    <SplitPane dividerPositions="0.29797979797979796" prefHeight="160.0" prefWidth="200.0">
                        <items>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                                <children>
                                    <ListView fx:id="historyResultsListView" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                </children>
                            </AnchorPane>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
                        </items>
                    </SplitPane>
                </content>
            </Tab>
        </tabs>
    </TabPane>
</children>
</GridPane>
